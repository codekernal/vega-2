            <section id="section-portfolio" class="no-top no-bottom">
                <div class="container">

                    <div class="spacer-single"></div>

                    <!-- portfolio filter begin -->
                    <div class="row">
                        <div class="col-md-12 text-center">
                            <ul id="filters" class="wow fadeInUp" data-wow-delay="0s">
                                <li><a href="#" data-filter="*" class="selected">All Projects</a></li>
                                {% for project_cat in project_cats %}
                                    <li><a href="#" data-filter=".{{project_cat.name}}">{{project_cat.name}}</a></li>
                                {% endfor %}
                            </ul>

                        </div>
                    </div>
                    <!-- portfolio filter close -->

                </div>

                <div id="gallery" class="gallery full-gallery de-gallery pf_full_width wow fadeInUp" data-wow-delay=".3s">

                    {% for project in projects %}
                    <!-- gallery item -->
                    <div class="item {{project.cat_name}}">
                        <div class="picframe">
                            <a class="" onclick="showProjectDetail({{project.id}});" href="javascript:void(0);">
                                <span class="overlay">
                                    <span class="pf_text">
                                        <span class="project-name">{{project.name}}</span>
                                    </span>
                                </span>
                            </a>

                            <img src="{{project.web_url}}" alt="" />
                        </div>
                    </div>
                    <!-- close gallery item -->

                    {% endfor %}

                </div>

                <div id="loader-area">
                    <div class="project-load"></div>
                </div>
            </section>


